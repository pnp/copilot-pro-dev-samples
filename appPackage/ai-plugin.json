{
    "$schema": "https://developer.microsoft.com/json-schemas/copilot/plugin/v2.1/schema.json",
    "schema_version": "v2.1",
    "name_for_human": "My Security Agent",
    "description_for_human": "Description for Users - Subset",
    "description_for_model": "Description for Users - Subset",
    "contact_email": "publisher-email@example.com",
    "namespace": "myagentall",
    "capabilities": {
        "conversation_starters": [
            {
                "text": "List a user's direct memberships"
            }
        ]
    },
    "functions": [
        {
            "name": "user_ListMemberGraphOPre",
            "description": "Get groups that the current user is a direct member of."
        },
                {
            "name": "user_ListOwnedGroups",
            "description": "Get groups where the current user is the owner."
        }   
    ],
    "runtimes": [
        {
            "type": "OpenApi",
            "auth": {
                "type": "OAuthPluginVault",
                "reference_id": "${{OAUTH2AUTHCODE_CONFIGURATION_ID}}"
            },
            "spec": {
                "url": "myagentall-openapi.yml"
            },
            "run_for_functions": [
                "user_ListMemberGraphOPre"
            ]
        }
    ]
}